{
  "name": "RRW",
  "version": "0.0.1",
  "description": "a Boilerplate for PC SPAs use react, redux, react-router",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "gfw": "npm i cross-env && cross-env CHROMEDRIVER_CDNURL=http://npm.taobao.org/mirrors/chromedriver PHANTOMJS_CDNURL=http://npm.taobao.org/mirrors/phantomjs npm install -r https://registry.npm.taobao.org",
    "dev": "cross-env NODE_ENV=development DEBUG=rrw* nodemon --exec webpack-dev-server --progress",
    "e2e": "cross-env NODE_ENV=test DEBUG=rrw* node ./test/e2e/runner.js --env firefox",
    "e2e:dev": "cross-env NODE_ENV=test DEBUG=rrw* nodemon ./test/e2e/runner.js",
    "unit": "cross-env NODE_ENV=test DEBUG=rrw* karma start ./test/unit/runner.js",
    "unit:dev": "npm run unit -- --watch",
    "lint": "eslint --max-warnings 10 .",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint src/themes/**/*.css",
    "test": "npm run lint && npm run lint:css && npm run unit && npm run e2e",
    "clean": "node ./cli/clean",
    "compile": "cross-env NODE_ENV=production DEBUG=rrw* node ./cli/compile",
    "build": "npm run test && npm run clean && npm run compile",
    "docs": "bash ./cli/docs.sh",
    "push": "bash ./cli/push.sh"
  },
